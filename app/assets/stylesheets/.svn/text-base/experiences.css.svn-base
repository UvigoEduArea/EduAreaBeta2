.course_state_color{
	width: 15px;
	height: 15px;
	background-color: gray;
	position: relative;
	left: 0;
	margin-top: 10px;
}

.course_state_color, .course_state_select{
	display: inline-block;
}

.course_state_color.beingPrepared{
	background-color: red !important;
}

.course_state_color.inProgress{
	background-color: orange !important;
}

.course_state_color.finished{
	background-color: green !important;
}

.record_title_description .jqte_record{
	width: 95%;
	margin: 5px 10px;
	height: 75%;
}

.record_title_description .jqte_record .jqte_record_editor{
	height: 90%;
}

.move_right{
	overflow: hidden;
}

.select_record_type{
	height: 70px;
	width: 30%;
	background-color: #4682B4;
	border-radius: 10px;
	padding: 0% 2% 0.5% 2%;
	position: relative;
	left: 32%;
	margin-bottom: 3%;
}

.record_icon{
	margin: 2% 2% 2% 2%;
	width: 7.5%;
	cursor: pointer;
}

.evidences, .annotations{
	position: absolute;
	color: white;
}

.evidences{
	left: 7.5%;
}

.annotations{
	left: 52%;
}

.record_box{
	margin-bottom: 1%;
	border: 1px solid black;
	border-radius: 10px;
	height: 330px;
}

.record_box label{
	display: inline;
}

.header_block{
	display: inline;
	position: relative;
	top: 5px;
	left: 10px;
}

.record_title_description, .record_content{
	position: relative;
	height:82%;
	border: solid 1px black;
	margin: 10px 8px 4px 8px;
	border-radius: 10px;
	width: 45%;
}

.record_title_description{
	height:82%;	
}

.record_content{
	padding: 0 0.5%;
}

.record_title, .record_content_snippet_input, /*.record_image_description,*/ .record_description/*, .record_video_description*/{
	border: none;
	margin: 10px 10px 8px 10px;
	width: 95%;
	height: 30px;
	font-size: 15px;
	background-color: white;
}


textarea.record_description{
	font-size: 12px;
	height: 72%;
}

.record_content_snippet_input{
	height: 15px;
	font-size: 11px;
}

.snippet_view{
	overflow: hidden;
	height: 80%;
	margin: 10px 10px;
}

