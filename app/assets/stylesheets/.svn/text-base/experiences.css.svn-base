.image_selector, .video_selector, .document_selector{
	position: absolute;
	background-color: rgba(70,130,180,0.6);
	cursor: pointer;
	bottom: 0;
	text-align: center;
	width: 50%;
	left: 25%;
}

.image_selector{
	
	/*bottom: -10%;*/
		
}

.record_video{
	max-width: 70%;
	max-height: 90%;
}

.record_document{
	width: 50%;
}

.jqte_record_editor{
	height: 91%;
}

.record_content .jqte_record{
	height: 90%;
	margin: 15px;
}
/*lo que est√° comentado es pq antes las imagenes y video tenian un input debajo que se ha quitado*/
.images_container{
	position: relative;
	height: 100%;
	width: 100%;
	padding: 0.5% 0%;
}

.record_image{
	position: relative;
	height: 95%;
}

.record_video, .record_document{
	margin: 2% auto;
	display: block;
}

.delete_record{
	height: 24px;
	margin-bottom: 3px;
	cursor: pointer;
}

.video_selector{
	/*left: 25%;
	bottom: 17.7%;
	/*bottom: 4.5%;
	width: 50%;*/
}

.users_bar{
	height: 100%;
	width: 12%;
	background-color: #1F3A4D;
	color: white;
	position: absolute;
	overflow-y: scroll;
	z-index: 1 !important;
}

.user_box{
	display: block;
	margin-top: 5px;
	cursor: pointer;
	background-color: #1F3A4D;
}

.user_box:hover{
	background-color: #32C3D6;
}

.user_box.user_selected{
	background-color: #32C3D6;
}


ul.dropdown.code_experience {
	background-color: #1F3A4D;
	color: white;
	margin-left: 0px;
	top: 26%;
	border-radius: 0px;	
	width: 468px;
}

ul.dropdown.code_experience input{
	height: 125px;
}

ul.dropdown.code_experience img{
	float: right;
}

#show_code_experience{
	width: 50%;
	height: 50%;
	position:absolute;
	left: 35%;
	top: 35%;
	background-color: #737D87;
	border-radius: 30px;
}

#show_code_experience span{
	color: white;
	position: relative;
	top: 24%;
	left: 9%;
}

#show_code_experience img{
	padding: 1%;
	margin-left: 91%;
}
